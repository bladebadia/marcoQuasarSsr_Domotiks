{
  "buildCommand": "quasar build -m ssr",
  "outputDirectory": "dist/ssr",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ],
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "dist/ssr/**"
    }
  }
}
